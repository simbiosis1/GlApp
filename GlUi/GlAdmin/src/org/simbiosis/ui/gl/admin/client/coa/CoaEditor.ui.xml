<!-- <!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent"> -->
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:v="urn:import:org.simbiosis.ui.gl.admin.client.editor"
	xmlns:ke="urn:import:org.kembang.module.client.editor"
	>
	<g:HTMLPanel>
		<table>
			<tr>
				<td class="formLabel">
					Kode COA
				</td>
				<td>
					<g:TextBox ui:field="code" styleName="formText"/>
				</td>
			</tr>
			<tr>
				<td class="formLabel">
					Prefix
				</td>
				<td>
					<g:TextBox ui:field="prefix" styleName="formText"/>
				</td>
			</tr>
			<tr>
				<td class="formLabel">
					Deskripsi
				</td>
				<td>
					<g:TextBox ui:field="description" styleName="formText"/>
				</td>
			</tr>
			<tr>
				<td class="formLabel">
					Induk
				</td>
				<td>
					<v:CoaListEditor ui:field="parent" styleName="formCombobox"/>
				</td>
			</tr>
			<tr>
				<td class="formLabel">
					Cabang
				</td>
				<td>
					<ke:BranchListBox ui:field="branch" styleName="formCombobox"/>
				</td>
			</tr>
			<tr>
				<td class="formLabel">
					Kecuali cabang
				</td>
				<td>
					<ke:BranchListBox ui:field="excBranch" styleName="formCombobox"/>
				</td>
			</tr>
		</table>
	</g:HTMLPanel>
</ui:UiBinder> 