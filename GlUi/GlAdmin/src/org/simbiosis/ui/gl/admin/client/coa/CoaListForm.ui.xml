<!-- <!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent"> -->
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:v="urn:import:com.google.gwt.user.cellview.client">
	<ui:style>
	</ui:style>
	<g:DockLayoutPanel unit='PX'>
		<g:west size="350">
			<g:DockLayoutPanel unit='PX' styleName='Global-FlexTable-Outer'>
				<g:north size='25'>
					<g:HorizontalPanel width="100%" height="100%"
						styleName='Global-FlexTable-Header' verticalAlignment="ALIGN_MIDDLE">
						<g:Label>Daftar Coa</g:Label>
					</g:HorizontalPanel>
				</g:north>
				<g:center>
					<g:ScrollPanel>
						<v:CellTree ui:field='listTable' />
					</g:ScrollPanel>
				</g:center>
			</g:DockLayoutPanel>
		</g:west>
		<g:center>
			<g:VerticalPanel ui:field="coaEditor" styleName="formLeftSpacer10px"/>
		</g:center>
	</g:DockLayoutPanel>
</ui:UiBinder> 